    "import json \n",
    " \n",
    "# Đọc nội dung tệp với mã hóa UTF-8 \n",
    "with open('thongtin.json', 'r', encoding='utf-8') as f: \n",
    "    content = f.read() \n",
    " \n",
    "# Phân tích nội dung JSON từ chuỗi UTF-8 \n",
    "infor = json.loads(content) \n",
    " \n",
    "print(infor) \n",
    "print(type(infor)) \n"
   ]
